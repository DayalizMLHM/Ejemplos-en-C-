#include<stdio.h>
//MATRIZ GIRADA
int main(){
	
	int matriz[20][20], i, j, n;
	
	scanf("%d", &n);
	
	
	for(i=0; i<n; i++){
		
		for(j=0; j<n; j++)
		
			scanf("%d", &matriz[i][j]); 	
	}
	printf("La matriz es: \n");
	for(i=0; i<n; i++){
		
		for(j=n-1; j>=0; j--){
			printf("%d ", matriz[j][i]);
				
		}
		
			 printf("\n");	
	}
	return 0;}
